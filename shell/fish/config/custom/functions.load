function mfs
	set host $argv[1]
	set mpoint $argv[2];
	set port 22;

	set volname (basename $mpoint)

	sshfs -p $port $host $mpoint -oauto_cache,reconnect -o Ciphers=arcfour
end

function fish_title
	echo (prompt_pwd)
end

function lsl
	 ls -l (find (pwd) -maxdepth 1 -type l -print)
end

function gcd
	set res (git rev-parse --show-cdup)
	if [ $res != "" ]
	  cd $res
	end
end